{
  "name": "@fabric-ds/react",
  "version": "1.0.4",
  "repository": "git@github.com:fabric-ds/react.git",
  "license": "ISC",
  "type": "module",
  "exports": "./dist/npm/index.js",
  "module": "dist/npm/index.js",
  "main": "dist/npm/index.js",
  "files": [
    "dist/npm"
  ],
  "types": "dist/npm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "17.0.2"
  },
  "scripts": {
    "build:clean": "rm -rf dist",
    "build:eik": "node esbuild.mjs",
    "build:types": "tsc && sed 's+declare module \"index\"+declare module \"@fabric-ds/react\"+g' ./dist/npm/index.d.ts > ./dist/npm/updated.d.ts && mv ./dist/npm/updated.d.ts ./dist/npm/index.d.ts",
    "build:npm": "npx esbuild ./packages/index.ts --outdir=dist/npm --target=es2017 --bundle --sourcemap --format=esm --external:react --minify",
    "build": "npm run build:clean && npm run build:npm && npm run build:eik && npm run build:types",
    "docs:build": "vite build && cd site && cp index.html 404.html && cp ../CNAME .",
    "docs:dev": "vite",
    "dev": "start-storybook -p 9003 --ci",
    "format": "prettier --write . --ignore-path .gitignore",
    "lint": "npm run lint:format && npm run lint:eslint",
    "lint:eslint": "eslint . --ext ts,tsx,js,jsx,cjs --max-warnings 0 --ignore-path .gitignore",
    "lint:format": "prettier --check . --ignore-path .gitignore",
    "eik:publish": "npx @eik/cli publish",
    "eik:alias": "npx @eik/cli pkg-alias",
    "preversion": "npx @eik/cli login && npm run build",
    "postversion": "npm publish && npm run eik:publish && git push --follow-tags"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@chbphone55/classnames": "^2.0.0",
    "@eik/esbuild-plugin": "^1.1.0",
    "@fabric-ds/component-classes": "^0.0.35",
    "@fabric-ds/icons": "^0.3.12",
    "@fabric-ds/tailwind-config": "^0.5.20",
    "@headlessui/react": "^1.4.2",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "1.6.22",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vitejs/plugin-react-refresh": "^1.1.3",
    "babel-eslint": "^10.1.0",
    "d3-scale": "^4.0.2",
    "element-collapse": "^1.0.1",
    "esbuild": "^0.13.3",
    "eslint": "^7.18.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "microbundle": "^0.13.3",
    "prettier": "^2.2.1",
    "prism-react-renderer": "^1.1.1",
    "react": "16.14.0",
    "react-docgen-typescript": "^1.20.5",
    "react-dom": "16.14.0",
    "react-focus-lock": "^2.5.2",
    "react-router-dom": "^5.2.0",
    "react-spring": "^8.0.27",
    "react-use-gesture": "^9.1.3",
    "rehype-stringify": "^8.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remark-slug": "^6.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "scroll-doctor": "^0.9.1",
    "typescript": "4.2.2",
    "vite": "^2.3.3",
    "vite-plugin-mdx": "3.5.6"
  },
  "dependencies": {
    "@chbphone55/classnames": "^2.0.0",
    "d3-scale": "^4.0.2",
    "react-focus-lock": "^2.5.2",
    "react-spring": "^8.0.27",
    "react-use-gesture": "^9.1.3",
    "resize-observer-polyfill": "^1.5.1",
    "scroll-doctor": "^0.9.1"
  },
  "eik": {
    "server": "https://assets.finn.no",
    "type": "package",
    "files": "./dist/eik",
    "import-map": [
      "https://assets.finn.no/map/react/v2"
    ]
  }
}
